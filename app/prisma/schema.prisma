generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserSharedPlaylist {
  user_shared_playlist_id Int @id @default(autoincrement())
  playlist_id             Int
  user_id                 Int
}

model UserSharedMusic {
  user_shared_music_id Int @id @default(autoincrement())
  music_id             Int
  user_id              Int
}

model UserMusicPlatform {
  user_music_platform Int    @id @default(autoincrement())
  user_id             Int
  token_account       String
  music_media_id      Int
}

model MusicPlaform {
  music_platform   Int    @id @default(autoincrement())
  music_media_id   Int
  music_media_name String
}
